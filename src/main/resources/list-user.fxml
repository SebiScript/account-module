<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.VBox?>
<VBox prefHeight="400.0" prefWidth="575.0" xmlns="http://javafx.com/javafx/17.0.12" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.jala.university.presentation.controller.ListUserController">
    <children>
        <Pane prefHeight="395.0" prefWidth="475.0">
            <children>
                <TableView fx:id="usersTable" layoutX="30.0" layoutY="68.0" prefHeight="238.0" prefWidth="523.0">
                    <columns>
                        <TableColumn fx:id="username" prefWidth="120.0" text="Username" />
                        <TableColumn fx:id="role" editable="false" prefWidth="120.0" sortable="false" text="Role" />
                        <TableColumn fx:id="email" editable="false" maxWidth="1.7976931348623157E308" prefWidth="120.0" sortable="false" text="Email" />
                    </columns>
                </TableView>
                <Button layoutX="30.0" layoutY="35.0" mnemonicParsing="false" onAction="#onReturn" prefHeight="26.0" prefWidth="80.0" text="Back" />
                <Button layoutX="473.0" layoutY="355.0" mnemonicParsing="false" onAction="#onUpdate" prefHeight="26.0" prefWidth="80.0" text="Update" />
            </children>
        </Pane>
    </children>
</VBox>
